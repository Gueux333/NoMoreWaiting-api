<h1>Arounds</h1>

<table class="table">
 <thead>
   <th>idPlace</th>
   <th>Lat</th>
   <th>Lng</th>
   <th>Latp</th>
   <th>Lngp</th>
   <th>Distance</th>
 </thead>

  {{#list Arounds}}

  <tr>
    <td>
      <a class="btn btn-secondary btn-sm" href="/userupdates/id/{{id}}" role="button">
        {{idPlace}}
      </a>
    </td>
    <td>
      {{Lat}}
    </td>
    <td>
      {{Lng}}
    </td>
     <td>
      {{Latp}}
    </td>
    <td>
      {{Lngp}}
    </td>
    <td>
      {{Distance}}
    </td>
    <td>
      <a class="btn btn-outline-secondary btn-sm" href="/arounds/update/{{id}}" role="button">
        Update
      </a>
      <a class="btn btn-outline-danger btn-sm" href="/arounds/delete/{{id}}" role="button">
        Delete
      </a>
    </td>
  </tr>

  {{/list}}
</table>

<div class="text-center">
  <a class="btn btn-primary" href="/userupdates/create" role="button">
    Add an Around info
  </a>
</div>